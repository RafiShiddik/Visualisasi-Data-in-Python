      "source": [
        "# Menghitung IQR\n",
        "q1 = df_cal['Population'].quantile(0.25)\n",
        "q3 = df_cal['Population'].quantile(0.75)\n",
        "iqr = q3 - q1\n",
        "iqr"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "JBZwDSCG4M_L",
        "outputId": "2f90f371-1eb8-4e43-df56-501584cf42d2"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "938.0"
            ]
          },
          "metadata": {},
          "execution_count": 94
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Menghitung Batas bawah dan batas atas\n",
        "lower_bound = q1 - 1.5 * iqr\n",
        "upper_bound = q3 + 1.5 * iqr\n",
        "lower_bound, upper_bound"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "R01b0FOF4Wm-",
        "outputId": "5d381367-7a38-4ccf-ecee-2f21f511a0e3"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "(-620.0, 3132.0)"
            ]
          },
          "metadata": {},
          "execution_count": 95
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Ambil pada rentang batas bawah dan batas atas\n",
        "df_cal = df_cal[(df_cal['Population'] >= lower_bound) & (df_cal['Population'] <= upper_bound)]"
      ],
      "metadata": {
        "id": "4yEjhEjt4bUp"
      },
      "execution_count": null,
      "outputs": []
    },
